

CREATE TABLE IF NOT MAJESTIC_CISCOUNT_CSV_EXPORT (
    TARGET_URL TEXT, 
    SOURCE_URL TEXT,
    ANCHOR_TEXT TEXT,
    FIRST_FOUND_DATE DATE,
    FLAG_NO_FOLLOW
    FLAG_IMAGE_LINK
    FLAG_REDIRECT
    FLAG_FRAME
    FLAG_OLD_CRAWL
    FLAG_ALT_TEXT
    FALG_MENTION
    SOURCE_CITATIONFLOW
    SOURCE_TRUSTFLOW
    TARGET_CITATIONFLOW
    TARGET_TRUSTFLOW
    SOURCE_TOPICAL_TRUSTFLOW_TOPIC_0
    SOURCE_TOPICAL_TRUSTFLOW_VALUE_0 )
